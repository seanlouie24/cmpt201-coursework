#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <sys/types.h>

int main(void) {
  // 1. Receive user input -> full path command
  char *line = NULL;
  size_t n =0;

  printf("Enter programs to run.\n");
  getline(&line, &n, stdin);
  exec(line);
  
  free(line);
  // 2. Execute command -> fork () and exec()
  // 3. Do 1. and 2. always
  return 0;
}
